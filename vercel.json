{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "framework": "create-react-app",
  "functions": {
    "api/index.js": {
      "includeFiles": "server/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
